    "css-loader": "^3.2.0",
    "immutability-helper": "^3.0.1",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.1",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.3",
    "sass": "^1.35.1",
    "sass-loader": "^8.0.0",
    "serve": "^12.0.0",
    "sprintf-js": "^1.1.2",
